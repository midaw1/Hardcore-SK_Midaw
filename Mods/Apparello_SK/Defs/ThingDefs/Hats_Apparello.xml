<?xml version="1.0" encoding="utf-8" ?>
<ThingDefs>

  <ThingDef Name="ApparelBase" Abstract="True">
    <thingClass>Apparel</thingClass>
    <category>Item</category>
    <selectable>True</selectable>
    <pathCost>10</pathCost>
    <useHitPoints>True</useHitPoints>
    <graphicData>
      <onGroundRandomRotateAngle>35</onGroundRandomRotateAngle>
    </graphicData>
    <drawGUIOverlay>true</drawGUIOverlay>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>3</DeteriorationRate>
      <SellPriceFactor>0.5</SellPriceFactor>
    </statBases>
    <techLevel>Midworld</techLevel>
    <thingCategories>
      <li>Apparellohats</li>
    </thingCategories>
    <altitudeLayer>Item</altitudeLayer>
    <alwaysHaulable>True</alwaysHaulable>
    <tickerType>Never</tickerType>
    <comps>
      <li>
        <compClass>CompForbiddable</compClass>
      </li>
      <li>
        <compClass>CompColorable</compClass>
      </li>
      <li>
        <compClass>CompQuality</compClass>
      </li>
    </comps>
  </ThingDef>



  <!--Beret-->

  <ThingDef ParentName="ApparelBase">
    <defName>Apparello_Beret</defName>
    <label>Beret</label>
    <description>Soft and keeps the head warm. Very common amongst offduty soldiers. Uniforms commonly inspire respect.</description>
    <graphicData>
      <texPath>Hats/Display/Beret</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>25</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
    </stuffCategories>
    <statBases>
      <ArmorRating_Blunt>0.03</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.03</ArmorRating_Sharp>
      <Insulation_Cold>-1</Insulation_Cold>
      <Insulation_Heat>-1</Insulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <RecruitPrisonerChance>0.05</RecruitPrisonerChance>
      <SocialImpact>0.05</SocialImpact>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <worngraphicPath>Hats/Beret</worngraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <commonality>1</commonality>
      <tags>
        <li>Military</li>
        <li>Spacer</li>
	<li>BrotherhoodLight</li>
	<li>SectarianLight</li>
	<li>SyndicateLight</li>
	<li>OrionLight</li>
	<li>NightbladesLight</li>
	<li>PredatorsLight</li>
      </tags>
      <defaultOutfitTags>
        <li>Worker</li>
      </defaultOutfitTags>
    </apparel>
    <colorGenerator Class="ColorGenerator_Options">
      <options>
        <li>
          <weight>4</weight>
          <min>RGBA(0.3,0.3,0.3,1)</min>
          <max>RGBA(0.9,0.9,0.9,1)</max>
        </li>
      </options>
    </colorGenerator>
    <thingCategories>
      <li>ApparelloSetWarden</li>
    </thingCategories>
  </ThingDef>




