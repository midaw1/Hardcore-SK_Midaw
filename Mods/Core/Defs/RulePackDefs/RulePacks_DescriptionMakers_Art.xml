<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>ArtSculpture</defName>
    <include>
      <li>ArtUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>description->This [sculpture] [depicts] [image]</li>
        <li>description->This [sculpture] bears a [depiction] of [image]</li>
        <li>description->On this [sculpture] is a [depiction] of [image]</li>
        <li>sculpture->sculpture</li>
        <li>sculpture->work</li>
        <li>sculpture->piece</li>
        <li>sculpture->carving</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>

