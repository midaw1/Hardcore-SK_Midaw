<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <RulePackDef>
    <defName>TalelessImages</defName>
    <include>
      <li>ArtUtility</li>
    </include>
    <rulePack>
      <rulesStrings>
        <li>image->a [outlander] surrounded by [outlander]s. [part2]</li>
        <li>image->a [outlander] playing [game] with [quantity_adjphrase] [outlander]s. [part2]</li>
        <li>image->a [outlander] [subject_desc] holding a [weapon]. [part2]</li>
        <li>image->a solitary [outlander]. [part2]</li>
        <li>image->a [adjective] [outlander] wearing a [apparel]. [part2]</li>
        <li>image->an empty [apparel] [subject_desc]. [part2]</li>
        <li>image->a [adjective] [terrainfeature]. [part2]</li>
        <li>image->[animal]s playing [game]. [part2]</li>
        <li>image->a [animal] wearing a [apparel] [subject_desc]. [part2]</li>
        <li>part2->[artextra_sentence] [artextra_sentence]</li>
        <li>subject_desc->flying through the air</li>
        <li>subject_desc->suspended in the air</li>
        <li>subject_desc->standing on the ground</li>
        <li>subject_desc->moving to the left</li>
        <li>subject_desc->moving to the right</li>
        <li>subject_desc->sitting on a table</li>
        <li>subject_desc->seen through a looking glass</li>
        <li>subject_desc->rendered in silhouette</li>
        <li>subject_desc->riding on a [animal] wearing a [apparel]</li>
        <li>subject_desc->looking [adjective]</li>
      </rulesStrings>
    </rulePack>
  </RulePackDef>


</Defs>