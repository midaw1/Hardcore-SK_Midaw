  <RulePackDef>
    <defName>NamerArtUtility</defName>
    <rulePack>
      <rulesStrings>
        <li>trans->of</li>
        <li>trans->for</li>
        <li>trans->by</li>
        <li>trans->with</li>
      </rulesStrings>
      <rulesRaw>
        <li Class="Rule_File">
          <keyword>art_adjective</keyword>
          <pathList>
            <li>NameBanks/Adjectives_Badass</li>
            <li>NameBanks/Adjectives_Angsty</li>
            <li>NameBanks/Adjectives_Angsty</li>
          </pathList>
        </li>
        <li Class="Rule_File">
          <keyword>terrainfeature</keyword>
          <path>NameBanks/Nouns_TerrainFeatures</path>
        </li>
        <li Class="Rule_PersonName">
          <selectionWeight>25</selectionWeight>
          <keyword>personname</keyword>
        </li>
        <li Class="Rule_File">
          <keyword>artname</keyword>
          <path>NameBanks/Nouns_Artworks</path>
        </li>
        <li Class="Rule_File">
          <keyword>concept</keyword>
          <pathList>
            <li>NameBanks/Nouns_Concepts_Badass</li>
            <li>NameBanks/Nouns_Concepts_Positive</li>
            <li>NameBanks/Nouns_Concepts_Angsty</li>
            <li>NameBanks/Nouns_Concepts_Angsty</li>
          </pathList>
        </li>
      </rulesRaw>
    </rulePack>
  </RulePackDef>



</Defs>